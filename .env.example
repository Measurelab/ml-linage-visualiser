# Development password for login screen
# Used to protect the app in development/staging environments
VITE_DEV_PASSWORD=your_password_here

# Iframe Security Configuration
# Shared secret for HMAC authentication (required in production)
VITE_TOOL_SECRET=your-shared-secret-key-here

# Main site URL that's allowed to embed this tool
VITE_ALLOWED_ORIGIN=https://your-main-site.vercel.app

# Set to 'true' to require authentication in development
VITE_REQUIRE_AUTH=false

# Supabase configuration (optional)
# If not provided, the app will use CSV files from the public folder
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
